generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        Int      @id @default(autoincrement())
  uid       String   @unique @default(cuid(2))
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  account   String   @unique
  password  String
}
